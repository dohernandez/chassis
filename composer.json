{
  "name": "dohernandez/chassis",
  "description": "Simple php's application chassis based on open-sources packages.",
  "authors": [
    {
      "name": "Darien Hernandez",
      "email": "dohernandez@gmail.com"
    }
  ],
  "require": {
    "php": "~7.1",
    "symfony/http-foundation": "^3.0",
    "symfony/dependency-injection": "^3.3",
    "nikic/fast-route": "^1.0",
    "symfony/yaml": "^3.3",
    "symfony/config": "^3.3",
    "symfony/event-dispatcher": "^3.3",
    "symfony/console": "^3.3",
    "monolog/monolog": "^1.23",
    "league/fractal": "^0.16",
    "league/tactician": "^1.0.2",
    "doctrine/dbal": "^2.5.12",
    "doctrine/migrations": "~1.5.0",
    "justinrainbow/json-schema": "~5.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.3.2",
    "fzaninotto/faker": "^1.5.0",
    "fabpot/php-cs-fixer": "^1.11.2",
    "symfony/var-dumper": "^3.0.4"
  },
  "autoload": {
    "psr-4": {
      "Chassis\\": "src/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "config": {
    "optimize-autoloader": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}